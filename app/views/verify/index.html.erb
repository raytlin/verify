<% 
require 'net/http'
require 'uri'

url = URI.parse(@url)
res = Net::HTTP.start(url.host, url.port) {|http|
  http.get('/')
}
	newURI = URI.parse(res['location'])
	fixedURI = URI.parse(newURI.scheme + "://" + newURI.host + "/favicon.ico")

	response = Net::HTTP.get_response(fixedURI)
%>
<%= response.body %>
